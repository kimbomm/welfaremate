{
  "name": "welfaremate",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=@welfaremate/web",
    "dev:admin": "turbo dev --filter=@welfaremate/admin",
    "build": "turbo build",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "generate": "pnpm --filter=@welfaremate/generate-snapshot generate",
    "crawl:sample": "pnpm --filter=@welfaremate/crawl-detail crawl:sample",
    "crawl:full": "pnpm --filter=@welfaremate/crawl-detail crawl:full",
    "crawl:limit": "pnpm --filter=@welfaremate/crawl-detail crawl:limit",
    "enrich": "pnpm --filter=@welfaremate/enrich-from-detail enrich",
    "enrich:sample": "pnpm --filter=@welfaremate/enrich-from-detail enrich:sample"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.3.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20"
  }
}
